  "scripts": {
    "start": "npm run build:scene && concurrently -n \"scene,builder\" \"decentraland-compiler buildScene.json --watch\" \"npm run start:website\"",
    "start:website": "node scripts/start.js",
    "build:scene": "decentraland-compiler buildScene.json",
    "build:contracts": "typechain --target ethers-v5 --out-dir src/contracts src/contracts/*.json",
    "prebuild": "node scripts/prebuild.js",
    "build": "npm run postinstall && npm run prebuild && npm run build:scene && node scripts/build.js",
    "build:ci": "npm run lint && npm run build",
    "postbuild": "node scripts/postbuild.js",
    "test": "node scripts/test.js",
    "test:coverage": "npm run test -- --coverage",
    "lint": "tslint --project tsconfig.json -e '*.json' -c tslint.json 'src/**/*.ts{,x}'",
    "postinstall": "node ./scripts/postinstall.js",
    "bundle:svg": "npx svg-sprite -D src/components/Icon --css-dimensions \"\" --cs icons.svg --css --css-dest . --css-bust false --css-prefix \".Icon.%s\" --css-render-css src/icons/*.svg"
