  "scripts": {
    "postinstall": "node ./scripts/postinstall.cjs",
    "test": "jest --maxWorkers=2",
    "test:coverage": "npm run test -- --coverage",
    "eject": "react-scripts eject",
    "prebuild": "node scripts/prebuild.cjs",
    "build:website": "tsc && node --max_old_space_size=8192 ./node_modules/vite/bin/vite.js build",
    "build": "npm run postinstall && npm run prebuild && npm run build:scene && tsc && node --max_old_space_size=8192 ./node_modules/vite/bin/vite.js build",
    "build:ci": "npm run lint && npm run build",
    "preview": "vite preview",
    "start": "npm run build:scene && concurrently -n \"scene,builder\" \"npm run build:scene -- --watch\" \"npm run start:website\"",
    "start:website": "vite",
    "build:scene": "node --openssl-legacy-provider ./node_modules/.bin/decentraland-compiler buildScene.json",
    "bundle:svg": "npx svg-sprite -D src/components/Icon --css-dimensions \"\" --cs icons.svg --css --css-dest . --css-bust false --css-prefix \".Icon.%s\" --css-render-css src/icons/*.svg",
    "build:contracts": "typechain --target ethers-v5 --out-dir src/contracts src/contracts/*.json",
    "lint": "eslint -c .eslintrc.json --ext .ts src",
    "check:code": "eslint -c .eslintrc.json src",
    "check:prettier": "prettier --config .prettierrc.json -c 'src/**/*.{js,ts,json,yml,md,tsx,html,css}'",
    "fix:code": "npm run check:code -- --fix",
    "fix:prettier": "npm run check:prettier -- --write",
    "pre-commit:fix:code": "eslint -c .eslintrc.json --fix",
    "pre-commit:fix:prettier": "prettier --config .prettierrc.json --write",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/decentraland/builder.git"
